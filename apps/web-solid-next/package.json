{
  "name": "web-solid-next",
  "type": "module",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:codegen": "tsx ./wait.ts && npm run codegen -- -w src/routes/**/*.ts",
    "dev:vinxi": "vinxi dev --host",
    "codegen": "graphql-codegen --config codegen.yml",
    "build": "vinxi build",
    "start": "vinxi start",
    "check": "npm-run-all check:*",
    "check:lint": "eslint --ext .ts,.tsx .",
    "check:format": "prettier --check .",
    "check:ts": "tsc",
    "format": "prettier --write .",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@fontsource-variable/roboto-mono": "^5.0.17",
