    "pluralize": "^8.0.0",
    "postcss": "^8.4.26",
    "pretty-ms": "^9.0.0",
    "slugify": "^1.6.6",
    "solid-floating-ui": "^0.2.1",
    "solid-js": "^1.8.11",
    "solid-mdx": "^0.0.7",
    "tailwind-merge": "^2.2.0",
    "tailwindcss": "^3.3.3",
    "tiny-invariant": "^1.3.1",
    "video.js": "^8.9.0",
    "vinxi": "^0.1.4",
    "xss": "^1.0.14",
    "zod": "^3.22.4"
