    "@solidjs/router": "^0.10.9",
    "@solidjs/start": "^0.4.10",
    "@tabler/icons": "^2.46.0",
    "autoprefixer": "^10.4.14",
    "binary-search": "^1.3.6",
    "clsx": "^2.1.0",
    "copy-text-to-clipboard": "^3.2.0",
    "delay": "^6.0.0",
    "feed": "^4.2.2",
    "graphql-request": "^6.1.0",
    "human-format": "^1.2.0",
    "human-number": "^2.0.4",
    "is-ip": "^5.0.1",
    "just-debounce": "^1.1.0",
    "just-group-by": "^2.2.0",
