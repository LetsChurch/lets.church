{
  "name": "web-next",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:astro": "astro dev --host 0.0.0.0",
    "dev:codegen": "npm run codegen -- -w 'src/**/*.ts'",
    "start": "node ./dist/server/entry.mjs",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "codegen": "graphql-codegen --config codegen.yml",
    "check": "run-s check:*",
    "check:astro": "astro check --tsconfig ./tsconfig.json",
    "check:lint": "eslint --ext .ts,.tsx,.astro .",
    "check:ts": "tsc --noEmit",
    "check:prettier": "prettier --check ."
  },
  "dependencies": {
    "@astrojs/check": "^0.3.4",
    "@astrojs/mdx": "^2.0.2",
    "@astrojs/node": "^7.0.2",
    "@astrojs/solid-js": "^3.0.2",
    "@astrojs/tailwind": "^5.0.4",
    "@knpwrs/envariant": "^1.1.1",
    "@tabler/icons": "^2.44.0",
    "astro": "^4.0.7",
    "clsx": "^2.0.0",
    "date-fns": "^3.0.6",
    "graphql-request": "^6.1.0",
    "human-format": "^1.2.0",
    "is-ip": "^5.0.1",
    "pluralize": "^8.0.0",
    "solid-floating-ui": "^0.2.1",
    "solid-js": "^1.8.7",
    "tailwind-merge": "^2.1.0",
    "tailwindcss": "^3.3.6",
    "tiny-invariant": "^1.3.1",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/near-operation-file-preset": "^3.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@sentry/astro": "^7.91.0",
    "@tailwindcss/forms": "^0.5.7",
    "@types/node": "^20.10.5",
    "@types/pluralize": "^0.0.33",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-astro": "^0.31.0",
    "eslint-plugin-import": "npm:eslint-plugin-i@^2.29.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.1",
    "prettier-plugin-astro": "^0.12.2",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "vite-plugin-svg-sprite-components-core": "^1.0.0",
    "vite-plugin-svg-sprite-components-solid": "^1.0.0"
  }
}
