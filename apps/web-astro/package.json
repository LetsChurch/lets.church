{
  "name": "web-astro",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "astro dev --host 0.0.0.0",
    "dev:astro": "astro dev --host 0.0.0.0",
    "dev:codegen": "npm run codegen -- -w 'src/**/*.ts'",
    "start": "node ./dist/server/entry.mjs",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "codegen": "graphql-codegen --config codegen.yml",
    "check": "run-s check:*",
    "check:astro": "astro check --tsconfig ./tsconfig.json",
    "check:lint": "eslint --ext .ts,.tsx,.astro .",
    "check:ts": "tsc --noEmit",
    "check:prettier": "prettier --check .",
    "format": "prettier -w ."
  },
  "dependencies": {
    "@astrojs/check": "^0.4.1",
    "@astrojs/mdx": "^2.1.1",
    "@astrojs/node": "^8.1.0",
    "@astrojs/solid-js": "^4.0.1",
    "@astrojs/tailwind": "^5.1.0",
    "@fontsource-variable/roboto-mono": "^5.0.17",
    "@knpwrs/envariant": "^1.1.1",
    "@sentry/astro": "^7.99.0",
    "@solid-primitives/autofocus": "^0.0.109",
    "@solid-primitives/scheduled": "^1.4.2",
    "@tabler/icons": "^2.46.0",
    "astro": "^4.2.8",
    "clsx": "^2.1.0",
    "date-fns": "^3.3.1",
    "gql.tada": "^1.2.0",
    "graphql-request": "^6.1.0",
    "human-format": "^1.2.0",
    "is-ip": "^5.0.1",
    "mapbox-gl": "^3.1.2",
    "maplibre-gl": "^3.6.2",
    "pluralize": "^8.0.0",
    "protomaps-themes-base": "^2.0.0-alpha.4",
    "solid-floating-ui": "^0.3.1",
    "solid-js": "^1.8.12",
    "tailwind-merge": "^2.2.1",
    "tiny-invariant": "^1.3.1",
    "typescript": "^5.3.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@0no-co/graphqlsp": "^1.3.2",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/near-operation-file-preset": "^3.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@tailwindcss/forms": "^0.5.7",
    "@types/mapbox-gl": "^2.7.20",
    "@types/node": "^20.11.14",
    "@types/pluralize": "^0.0.33",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "eslint": "^8.56.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-astro": "^0.31.4",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-solid": "^0.13.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.4",
    "prettier-plugin-astro": "^0.13.0",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.7.0",
    "vite-plugin-svg-sprite-components-core": "^1.0.0",
    "vite-plugin-svg-sprite-components-solid": "^1.0.0"
  }
}
