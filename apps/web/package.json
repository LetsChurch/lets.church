{
  "name": "web-solid",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:solid": "solid-start dev",
    "dev:codegen": "ts-node-esm --transpileOnly ./wait.ts && npm run codegen -- -w src/routes/**/*.ts",
    "build": "solid-start build",
    "start": "solid-start start",
    "check": "npm-run-all check:*",
    "check:lint": "eslint --ext .ts,.tsx .",
    "check:format": "prettier --check .",
    "check:ts": "tsc",
    "codegen": "graphql-codegen --config codegen.yml",
    "postinstall": "patch-package"
  },
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@fontsource/inter": "^4.5.14",
    "@knpwrs/envariant": "^1.0.2",
    "@solid-primitives/scheduled": "^1.2.0",
    "@solidjs/meta": "^0.28.2",
    "@solidjs/router": "^0.5.1",
    "@tabler/icons": "^1.113.0",
    "graphql-request": "^5.0.0",
    "p-progress": "^0.6.0",
    "p-retry": "^5.1.2",
    "solid-floating-ui": "^0.2.0",
    "solid-js": "^1.6.2",
    "solid-start": "^0.2.6",
    "tiny-invariant": "^1.3.1",
    "undici": "^5.11.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.14.0",
    "@graphql-codegen/near-operation-file-preset": "^2.4.4",
    "@graphql-codegen/typescript": "^2.8.2",
    "@graphql-codegen/typescript-operations": "^2.5.7",
    "@tailwindcss/forms": "^0.5.3",
    "@tsconfig/node18-strictest-esm": "^1.0.1",
    "@types/node": "^18.11.9",
    "@types/wait-on": "^5.3.1",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "autoprefixer": "^10.4.12",
    "eslint": "^8.28.0",
    "eslint-plugin-solid": "^0.9.1",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.5.0",
    "postcss": "^8.4.18",
    "prettier": "^2.8.0",
    "prettier-plugin-tailwindcss": "^0.2.0",
    "solid-start-node": "^0.2.6",
    "tailwindcss": "^3.2.0",
    "typescript": "^4.9.3",
    "vite": "^3.1.8",
    "vite-plugin-solid-svg": "^0.6.0",
    "wait-on": "^6.0.1"
  }
}
