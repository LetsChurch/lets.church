{
  "name": "web-solid",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:solid": "solid-start dev",
    "dev:codegen": "ts-node-esm --transpileOnly ./wait.ts && npm run codegen -- -w src/routes/**/*.ts",
    "build": "solid-start build",
    "start": "node ./dist/server.js",
    "check": "npm-run-all check:*",
    "check:lint": "eslint --ext .ts,.tsx .",
    "check:format": "prettier --check .",
    "check:ts": "tsc",
    "codegen": "graphql-codegen --config codegen.yml",
    "postinstall": "patch-package"
  },
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@fontsource/inter": "^4.5.15",
    "@fontsource/roboto-mono": "^4.5.10",
    "@knpwrs/envariant": "^1.1.1",
    "@solid-primitives/autofocus": "^0.0.105",
    "@solid-primitives/scheduled": "^1.3.2",
    "@solidjs/meta": "^0.28.2",
    "@solidjs/router": "^0.8.2",
    "@tabler/icons": "^1.116.0",
    "binary-search": "^1.3.6",
    "blurhash": "^2.0.5",
    "copy-text-to-clipboard": "^3.0.1",
    "graphql-request": "^5.2.0",
    "human-number": "^2.0.1",
    "p-progress": "^0.6.0",
    "p-retry": "^5.1.2",
    "pretty-ms": "^8.0.0",
    "solid-floating-ui": "^0.2.1",
    "solid-js": "^1.7.0",
    "solid-start": "^0.2.24",
    "tiny-invariant": "^1.3.1",
    "undici": "^5.20.0",
    "video.js": "^8.0.4",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^3.2.2",
    "@graphql-codegen/near-operation-file-preset": "^2.5.0",
    "@graphql-codegen/typescript": "^3.0.2",
    "@graphql-codegen/typescript-operations": "^3.0.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tsconfig/node18-strictest-esm": "^1.0.1",
    "@types/human-number": "^1.0.0",
    "@types/node": "^18.14.6",
    "@types/wait-on": "^5.3.1",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.35.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-solid": "^0.12.0",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.5.1",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.6",
    "solid-start-cloudflare-pages": "^0.2.24",
    "solid-start-cloudflare-workers": "^0.2.24",
    "solid-start-node": "^0.2.24",
    "tailwindcss": "^3.3.1",
    "type-fest": "^3.6.1",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vite-plugin-solid-svg": "^0.6.1",
    "wait-on": "^7.0.1"
  }
