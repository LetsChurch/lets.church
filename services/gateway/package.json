{
  "name": "@letschurch/gateway",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development node --watch --loader ts-node/esm --experimental-specifier-resolution=node src/server.ts",
    "dev:process-upload-worker": "NODE_ENV=development node --watch --loader ts-node/esm --experimental-specifier-resolution=node src/temporal/process-upload-worker.ts",
    "dev:background-worker": "NODE_ENV=development node --watch --loader ts-node/esm --experimental-specifier-resolution=node src/temporal/background-worker.ts",
    "start": "NODE_ENV=production node --loader ts-node/esm --experimental-specifier-resolution=node src/server.ts",
    "start:process-upload-worker": "NODE_ENV=production node --loader ts-node/esm --experimental-specifier-resolution=node src/temporal/process-upload-worker.ts",
    "start:background-worker": "NODE_ENV=production node --loader ts-node/esm --experimental-specifier-resolution=node src/temporal/background-worker.ts",
    "check": "npm-run-all check:*",
    "check:format": "prettier --check src prisma/seed.ts",
    "check:lint": "eslint --ext .ts,.tsx src",
    "check:ts": "tsc",
    "test": "vitest",
    "prisma:studio": "prisma studio",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:test": "./test-cmd prisma db push --accept-data-loss",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:migrate:status": "prisma migrate status",
    "prisma:migrate:reset": "prisma migrate reset",
    "prisma:db:push": "prisma db push",
    "prisma:db:push:force": "prisma db push --accept-data-loss",
    "prisma:db:seed": "prisma db seed",
    "prisma:db:truncate": "ts-node-esm --experimental-specifier-resolution=node prisma/truncate.ts",
    "prisma:generate": "prisma generate",
    "es:push-mappings": "ts-node-esm --experimental-specifier-resolution=node elasticsearch/mappings.ts"
  },
  "prisma": {
    "seed": "node --loader ts-node/esm --experimental-specifier-resolution=node prisma/seed.ts"
  },
  "license": "UNLICENSE",
