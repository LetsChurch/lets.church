{
  "name": "@letschurch/gateway",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:server": "NODE_ENV=development node --watch --loader ts-node/esm --experimental-specifier-resolution=node src/server.ts",
    "dev:background-worker": "NODE_ENV=development node --watch --loader ts-node/esm --experimental-specifier-resolution=node src/temporal/workers/background-worker.ts",
    "dev:transcode-worker": "NODE_ENV=development node --watch --loader ts-node/esm --experimental-specifier-resolution=node src/temporal/workers/transcode-worker.ts",
    "dev:transcribe-worker": "NODE_ENV=development node --watch --loader ts-node/esm --experimental-specifier-resolution=node src/temporal/workers/transcribe-worker.ts",
    "start:server": "NODE_ENV=production node --loader ts-node/esm --experimental-specifier-resolution=node src/server.ts",
    "start:background-worker": "NODE_ENV=production node --loader ts-node/esm --experimental-specifier-resolution=node src/temporal/workers/background-worker.ts",
    "start:transcode-worker": "NODE_ENV=production node --loader ts-node/esm --experimental-specifier-resolution=node src/temporal/workers/transcode-worker.ts",
    "start:transcribe-worker": "NODE_ENV=production node --loader ts-node/esm --experimental-specifier-resolution=node src/temporal/workers/transcribe-worker.ts",
    "check": "npm-run-all check:*",
    "check:format": "prettier --check src prisma/seed.ts",
    "check:lint": "eslint --ext .ts,.tsx src",
    "check:ts": "tsc",
    "test": "vitest",
    "prisma:studio": "prisma studio",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:test": "./test-cmd prisma db push --accept-data-loss",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:migrate:status": "prisma migrate status",
    "prisma:migrate:reset": "prisma migrate reset",
    "prisma:db:push": "prisma db push",
    "prisma:db:push:force": "prisma db push --accept-data-loss",
    "prisma:db:seed": "prisma db seed",
    "prisma:db:truncate": "ts-node-esm --experimental-specifier-resolution=node prisma/truncate.ts",
    "prisma:generate": "prisma generate",
    "es:push-mappings": "ts-node-esm --experimental-specifier-resolution=node elasticsearch/mappings.ts",
    "admin:create-user": "ts-node-esm --experimental-specifier-resolution=node scripts/create-user.ts",
    "admin:create-channel": "ts-node-esm --experimental-specifier-resolution=node scripts/create-channel.ts",
    "admin:delete-upload": "ts-node-esm --experimental-specifier-resolution=node scripts/delete-upload.ts",
    "admin:import": "ts-node-esm --experimental-specifier-resolution=node scripts/import.ts",
    "admin:reindex": "ts-node-esm --experimental-specifier-resolution=node scripts/reindex.ts",
    "admin:verify-uploads": "ts-node-esm --experimental-specifier-resolution=node scripts/verify-uploads.ts"
  },
  "prisma": {
    "seed": "node --loader ts-node/esm --experimental-specifier-resolution=node prisma/seed.ts"
  },
  "license": "UNLICENSE",
